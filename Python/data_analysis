import pandas as pd
import matplotlib.pyplot as plt

# Step 1: Read the CSV file into a DataFrame
df = pd.read_csv('data_log.csv')

# Step 2: Calculate the mean of each column
means = df.mean()

# Step 3: Store the means in a list
means_list = means.tolist()

# Step 4: Plot the means as a plot
plt.figure(figsize=(10, 6))
plt.plot(means.index, means_list, marker='o')
plt.xlabel('Columns')
plt.ylabel('Mean')
plt.title('Mean of Each Column')
plt.xticks(rotation=45) # Rotate x-axis labels for better readability
plt.grid(True)
plt.show()
